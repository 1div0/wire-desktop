{
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "es2016", "es2017.object"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "electron/dist",
    "removeComments": true,
    "rootDir": "electron/src",
    "sourceMap": true,
    "strict": true,
    "jsx": "react",
    "target": "es5"
  },
  "exclude": ["babel.config.js", "electron/dist", "electron/renderer", "electron/node_modules", "Gruntfile.js", "node_modules", "tests", "webpack.config.js", "wrap"]
}
