{
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["dom", "es2016", "es2017.object"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "dist",
    "removeComments": true,
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  },
  "exclude": ["dist"]
}
